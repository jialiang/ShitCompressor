{
  "OutputFolder": "optimized",
  "OutputSuffix": "(optimized)",
  "UseButteraugli": false,
  "EncoderSettings": [
    {
      "Name": "guetzli",
      "Signature": "--quality {Quality} {InputP} {OutputP}",
      "Filename": "guetzli.exe",
      "DefaultQuality": 95,
      "QualityMin": 50,
      "QualityMax": 100,
      "DefaultEnabled": false,
      "Input": [ "jpg", "png" ],
      "Output": "jpg",
      "Special": false
    },
    {
      "Name": "mozjpeg",
      "Signature": "{InputP} {OutputP} {Quality}",
      "Filename": "cjpeg.bat",
      "DefaultQuality": 95,
      "QualityMin": 50,
      "QualityMax": 100,
      "DefaultEnabled": true,
      "Input": [ "jpg", "png" ],
      "Output": "jpg",
      "Special": false
    },
    {
      "Name": "jpegtran",
      "Signature": "{InputP} {OutputP}",
      "Filename": "jpegtran.exe",
      "DefaultQuality": 0,
      "QualityMin": 0,
      "QualityMax": 0,
      "DefaultEnabled": true,
      "Input": [ "jpg" ],
      "Output": "jpg",
      "Special": false
    },
    {
      "Name": "jpegoptim",
      "Signature": "{InputP} {OutputP} --strip-all --force --max={Quality}",
      "Filename": "jpegoptim.bat",
      "DefaultQuality": 95,
      "QualityMin": 50,
      "QualityMax": 100,
      "DefaultEnabled": true,
      "Input": [ "jpg" ],
      "Output": "jpg",
      "Special": false
    },
    {
      "Name": "webp",
      "Signature": "-q {Quality} {InputP} -o {OutputP}",
      "Filename": "cwebp.exe",
      "DefaultQuality": 95,
      "QualityMin": 50,
      "QualityMax": 100,
      "DefaultEnabled": true,
      "Input": [ "jpg", "png" ],
      "Output": "webp",
      "Special": true
    },
    {
      "Name": "avif",
      "Signature": "--min 0 --max 63 -a end-usage=q -a cq-level={Quality} -a tune=ssim {InputP} {OutputP}",
      "Filename": "avifenc.exe",
      "DefaultQuality": 5,
      "QualityMin": 1,
      "QualityMax": 63,
      "DefaultEnabled": true,
      "Input": [ "jpg", "png" ],
      "Output": "avif",
      "Special": true
    }
  ]
}
